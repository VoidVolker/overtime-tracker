<h2 class="uk-heading-bullet">Audit Log</h2>
<table class="uk-table uk-table-striped uk-table-hover uk-table-small">
  <thead>
    <tr>
      <th>#</th>
      <th>Employee</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <%= render @audit_logs %>
  </tbody>
</table>

<%= paginate @audit_logs %>
